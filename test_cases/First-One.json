{
    "name": "First One",
    "integrationType": "custom-rest-actions",
    "actionType": "custom",
    "config": {
      "request": {
        "requestUrlTemplate": "${input.webhookUrl}",
        "requestType": "POST",
        "headers": {},
        "requestTemplate": "{\n \"message\": \"${input.message}\",\n \"metadata\": \"$!{input.metadata}\"\n}"
      },
      "response": {
        "translationMap": {},
        "translationMapDefaults": {},
        "successTemplate": "${rawResult}"
      }
    },
    "contract": {
      "input": {
        "inputSchema": {
          "type": "object",
          "properties": {
            "webhookUrl": {
              "type": "string"
            },
            "message": {
              "type": "string"
            },
            "metadata": {
              "type": "string"
            }
          },
          "additionalProperties": true
        }
      },
      "output": {
        "successSchema": {
          "type": "object",
          "properties": {},
          "additionalProperties": true
        }
      }
    },
    "secure": false
  }